{
  "expo": {
    "name": "Black Pill",
    "slug": "blackpill",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "description": "Be Honest About Yourself - AI-powered attractiveness analysis with actionable self-improvement tips.",
    "newArchEnabled": true,
    "scheme": "blackpill",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F1E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blackpill.blackpill",
      "infoPlist": {
        "NSCameraUsageDescription": "Black Pill uses your camera to take selfies so our AI can analyze your face and provide personalized attractiveness scores and improvement suggestions.",
        "NSPhotoLibraryUsageDescription": "Black Pill uses your photo library so you can select existing selfies for AI analysis and personalized attractiveness feedback.",
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "blackpill"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.blackpill.blackpill",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F1E"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "blackpill"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Black Pill to use your camera to take selfies so our AI can analyze your face and provide personalized attractiveness feedback and improvement tips."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Black Pill to access your photos so you can choose existing selfies for AI analysis and personalized attractiveness feedback."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF0080",
          "sounds": []
        }
      ],
      "expo-dev-client",
      "expo-secure-store",
      "expo-web-browser",
      "expo-apple-authentication",
      "./plugins/with-storekit-config.js"
    ],
    "extra": {
      "eas": {
        "projectId": "44e23637-a48c-478e-8017-a7bdbffc2231"
      }
    },
    "owner": "george11642"
  }
}
